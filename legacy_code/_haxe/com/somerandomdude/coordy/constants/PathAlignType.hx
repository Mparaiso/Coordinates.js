package com.somerandomdude.coordy.constants;

class PathAlignType
{
	/**
	 * Nodes rotation is parallel to the layout's path
	 */
	public static inline var  ALIGN_PARALLEL:String="alignParallel";
	
	/**
	 * Nodes rotation is perpendicular to the layout's path
	 */
	public static  inline var  ALIGN_PERPENDICULAR:String="alignPerpendicular";
	
	/**
	 * Nodes do not alter rotation to the layout's path
	 */
	public static  inline var  NONE:String="noAlign";
}
